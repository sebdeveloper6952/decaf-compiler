class Program
{
    int array[10];

    int find_min()
    {
        int min_num;
        int min_index;
        int i;

        min_num = array[0];

        i = 1;
        while (i < 10)
        {
            if (min_num <= array[i])
            {
                min_num = array[i];
                min_index = i;
            }

            i = i + 1;
        }

        return min_index;
    }

    void main()
    {
        int min;

        array[0] = 9;
        array[1] = 2;
        array[2] = 4;
        array[3] = 7;
        array[4] = 5;
        array[5] = 3;
        array[6] = 6;
        array[7] = 8;
        array[8] = 0;
        array[9] = 1;

        min = find_min();
    }
}