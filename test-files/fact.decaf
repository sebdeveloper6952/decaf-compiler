class Program
{
    int fact(int a)
    {
        int t;

        if (a <= 1)
        {
            return 1;
        }

        t = fact(a - 1);

        return a * t;
    }

    int read_int() { return 1; }
    void print_int(int a) {}

    void main()
    {
        int x;
        int res;

        x = read_int();

        res = fact(x);

        print_int(res);
    }
}